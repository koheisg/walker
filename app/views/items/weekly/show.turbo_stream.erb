<%= turbo_stream.append(:items) do %>
  <%= render partial: 'items/item', collection: @page.records, formats: :html %>
<% end %>

<%= turbo_stream.replace(:paginate_link) do %>
  <%= render partial: 'paginate_link', formats: :html, locals: { page: @page, link_path: weekly_path(format: 'turbo_stream', page: @page.next_param) } %>
<% end %>
