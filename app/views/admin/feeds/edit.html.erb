<% content_for :breadcrumb do %>
  <%= render('shared/breadcrumb', links: [
    {path: admin_feeds_path, name: 'Feeds'},
    {path: admin_feed_path(@feed), name: @feed.name},
    {path: request.path, name: :edit},
  ]) %>
<% end %>

<h1>Editing Feed</h1>

<%= render 'form', feed: @feed %>

<%= link_to 'Show', [:admin, @feed] %> |
<%= link_to 'Back', admin_feeds_path %>
